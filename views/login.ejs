<%- include('componentLayout/menu') %> 

<h2 class="page-title text-uppercase text-center mt-5 font-weight-bold text-dark">Login</h2>

<form class="container-sm ml-1" action="/login" method="post">
    <div class="form-group">
        <label for="">Email</label>
        <input type="email" name="email" id="" value="" class="form-control" placeholder="Enter email" required="true" >
    </div>
    <div class="form-group">
        <label for="">Password</label>
        <input type="password" name="password" id="" value="" class="form-control" placeholder="Enter password" required="true">
    </div>
    
    <button class="btn btn-dark w-100 pt-3 pb-3 font-weight-bold">Submit</button>
</form>


