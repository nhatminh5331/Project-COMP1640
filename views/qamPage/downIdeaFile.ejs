<%- include('../componentLayout/qamMenu') %> 
 <h1 class="mt-5 d-flex justify-content-center">Ideas data</h1>
 <br>

 <a class="text-info font-weight-bold" href="/files/idea.csv" download="Example_file.csv">IDEA</a>
 <br>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Title</th>
            <th>Topic</th>
            <th>Status</th>
            <th>Feature</th>
        </tr>
    </thead>
    <tbody>
        <% if(campaigns != ''){%> <% campaigns.forEach(function(campaign, index){ %>
          <% if(campaign.final_closure <= Date.now()){ %> 
              <tr>
                <td><%- index %></td>
                <td><%- campaign.title %></td>
                <td><%- campaign.name %></td>
                <td style="color: rgb(92, 92, 92);">Close</td>
                <td>
                  <a href="/qamPage/download_zip/<%= campaign._id %>">Download files</a>
                </td>
              </tr>
              <% } %> 
              <% }) %> 
        <% } %>
    </tbody>
</table>
<%- include('../componentLayout/footer')  %> 
